{
    "parserOption":{
        "ecmaVersion":2020,
        "sourceType":"module",
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "env":{
        "browser": true,
        "node":true,
        "es6":true
    },
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "plugins":[
        "import",
        "react-hooks",
        "prettier"
    ],
    "rules":{
        "prettier/prettier" : "error"
    }
    
}